    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^36.3.1",
    "electron-devtools-installer": "^4.0.0",
    "electron-playwright-helpers": "^1.7.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.1.0",
    "tailwindcss": "^4.1.7",
